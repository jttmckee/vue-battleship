{
  "name": "vue-battleship",
  "description": "Testing practive with Vue and Jest",
  "version": "1.0.0",
  "author": "jttmckee <wp.jason@hotmail.co.uk>",
  "license": "All Rights Reserved",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open  '/Applications/Firefox Developer Edition.app' --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest --watch"
  },
  "dependencies": {
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 0.25%"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }

}
